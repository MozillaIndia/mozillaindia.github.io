{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": 
	{
    		"bulma": "^0.7.5",
    		"del-cli": "^3.0.0",
    		"gatsby": "^2.13.2",
    		"gatsby-image": "^2.2.4",
    		"gatsby-plugin-manifest": "^2.2.1",
    		"gatsby-plugin-offline": "^2.2.0",
    		"gatsby-plugin-prefetch-google-fonts": "^1.4.2",
    		"gatsby-plugin-react-helmet": "^3.1.0",
    		"gatsby-plugin-sharp": "^2.2.2",
   		"gatsby-source-filesystem": "^2.1.2",
    		"gatsby-transformer-json": "^2.2.0",
    		"gatsby-transformer-sharp": "^2.2.1",
    		"prettier": "^1.18.2",
    		"prop-types": "^15.7.2",
    		"react": "^16.8.6",
    		"react-dom": "^16.8.6",
    		"react-helmet": "^5.2.1",
    		"react-icons": "^3.7.0",
    		"styled-components": "^4.3.2"
  	},
  "devDependencies": 
	{
    		"chalk": "^2.4.1",
    		"eslint": "^5.6.0",
    		"eslint-config-airbnb": "^17.1.0",
    		"eslint-config-prettier": "^4.1.0",
    		"eslint-plugin-import": "^2.14.0",
    		"eslint-plugin-jsx-a11y": "^6.1.1",
    		"eslint-plugin-react": "^7.12.3",
    		"gh-pages": "^2.0.1",
    		"husky": "^1.3.1",
    		"lint-staged": "^8.1.0"
  	},
  "husky": 
	{
    		"hooks": 
		{
      		  "pre-commit": "lint-staged"
    		}
  	},
  "lint-staged": 
	{
          	"*.{js,json,css,md}": 
			[
      			   "prettier --write",
      			   "git add"
    			]
  	},
  "keywords":
	 [
    	   "gatsby"
  	 ],
  "license": "MIT",
  "scripts": 
	{
    		"build": "del-cli ./.cache && gatsby build",
    		"develop": "del-cli ./.cache && gatsby develop",
    		"start": "yarn run develop",
    		"lint": "eslint ./src",
    		"setup": "node ./bin/setup.js",
    		"deploy:github": "node ./scripts/deploy-github",
    		"deploy": "yarn run build && yarn run deploy:github"
  	},
  "repository": 
	{
    		"type": "git",
    		"url": "https://github.com/gatsbyjs/gatsby-starter-default"
  	},
  "bugs":
	{
    		"url": "https://github.com/gatsbyjs/gatsby/issues"
  	}
}
